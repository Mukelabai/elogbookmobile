        <templates:DrawerTitleView/>
    </NavigationPage.TitleView>
    <ContentPage.Content>
        <StackLayout>
            <WebView WidthRequest="1000" HeightRequest="1000" Source="https://elogbook.coinfomas.net"></WebView>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>